[ req ]
default_bits       = 2048
prompt             = no
default_md        = sha256
req_extensions     = req_ext
distinguished_name = dn

[ dn ]
C  = AU                              
ST = State                           
L  = City                      
O  = Organization              
OU = Organizational Unit
emailAddress = your_email@example.com 

[ req_ext ]
subjectAltName = @alt_names  # Use this to refer to multiple SANs

[ alt_names ]
DNS.1 = accuknox-multi-cluster-helper-webhook.agents.svc
# Add more DNS entries if needed, for example:
# DNS.2 = another.dns.name.if.required